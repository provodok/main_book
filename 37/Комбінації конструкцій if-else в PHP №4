<?php
  $age = 92;

  if ($age < 10 or $age >99) {
    echo '$age менше 10 або більше 99';
  } elseif ($age >= 10 and $age<=99) {
      $age = (string)$age;
      $sum = 0;
      for ($i = 0; $i < strlen($age); $i++) {
      $sum += $age[$i];
    }
      if ($sum <=9) {
      echo 'сума цифр однозначна';
    } else {
      echo 'сума цифр двозначна';  
    }
  }
?>
    
